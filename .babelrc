{
  "presets": [
    "react",
    [
      "env", {
      "modules": false,
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }]
  ],
  "plugins": [
    [
      "react-css-modules",
      {
        "webpackHotModuleReloading": true,
        "generateScopedName": "[name]__[local]__[hash:base64:5]",
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss",
            "plugins": ["postcss-nested"]
          }
        }
      }
    ],
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}