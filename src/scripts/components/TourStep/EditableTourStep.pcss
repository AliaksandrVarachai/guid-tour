.editable {
  overflow: hidden;
  white-space: nowrap;
}

.editable-name {
  box-sizing: border-box;
  width: 100%;
  height: 20px;
  margin-right: 40px;
}

.editable-action-panel {
  padding-left: 10px;
  color: #606060;
}

.editable-action {
  &:global(.material-icons) {
    font-size: 1.4em;
  }

  &:hover {
    cursor: pointer;
  }
}

.is-wrong-value {
  border-color: red;
}

.tooltip {
  @mixin tooltip;
}
